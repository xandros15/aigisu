{
  "name": "xandros15/aigisu",
  "authors": [
    {
      "name": "xandros15",
      "email": "xandros@xandros.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "slim/slim": "^3.0",
    "illuminate/database": "5.2.*",
    "google/apiclient": "1.*",
    "plasticbrain/php-flash-messages": "^1.0",
    "illuminate/validation": "5.2.*",
    "illuminate/translation": "5.2.*",
    "xandros15/slim-pagination": "0.*"
  },
  "require-dev": {
    "phpspec/phpspec": "^3.0",
    "symfony/var-dumper": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "": "src",
      "Aigisu\\": [
        "src",
        "src/components/core"
      ],
      "Imgur\\": "src/components/imgur/imgur-php-wrapper",
      "Aigisu\\Components\\": "src/components"
    },
    "psr-0": {
      "": "src/"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
